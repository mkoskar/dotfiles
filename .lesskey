#command

h       left-scroll
l       right-scroll
\eh     no-scroll
\el     end-scroll
\40     undo-hilite
\e?     help

#line-edit

^B      home
^E      end
\ef     word-right
^W      word-backspace
^P      up
^N      down

#env

# enter_bold_mode / bold / md
LESS_TERMCAP_md = [1;97m

# enter_underline_mode / smul / us
LESS_TERMCAP_us = [4;93m
LESS_TERMCAP_ue = [0m

# enter_standout_mode / smso / so
LESS_TERMCAP_so = [1;41;37m
LESS_TERMCAP_se = [0m
